{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": true,
    "context": true,
    "expect": true,
    "_": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "angular/di": 0,
    "angular/no-service-method": 0
  }
}
